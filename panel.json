{
    // There can be C-like comments
    "layout": {
        "type": "grid",
        "rows": 6,
        "cols": 1,
        "vspace": "0mm",
        "hspace": "10mm",
        "rotation": "0deg",
        "alternation": "none",
        "renamenet": "Board_{n}-{orig}",
        "renameref": "{orig}"
    },
    "source": {
        "type": "auto",
        "tolerance": "5mm"
    },
    "tabs": {
        "type": "full",
    },
    "cuts": {
        "type": "vcuts"
    },
    "framing": {
        "type": "frame",
	    "hspace": "10mm",
        "vspace": "0mm",
	    "width": "5mm"
    }
}
