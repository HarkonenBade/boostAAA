{
    // There can be C-like comments
    "layout": {
        "type": "grid",
        "rows": 6,
        "cols": 1,
        "vspace": "2mm",
        "hspace": "0mm",
        "rotation": "0deg",
        "alternation": "none",
        "renamenet": "Board_{n}-{orig}",
        "renameref": "{orig}"
    },
    "source": {
        "type": "auto",
        "tolerance": "5mm"
    },
    "tabs": {
        "type": "fixed",
        "width": "40mm"
    },
    "cuts": {
        "type": "vcuts"
    },
    "framing": {
        "type": "none",
    }
}
